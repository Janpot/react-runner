{
  "name": "react-live-runner",
  "version": "0.5.0",
  "description": "Run your React code on the go",
  "homepage": "https://nihgwu.github.io/react-runner/",
  "repository": {
    "type": "git",
    "url": "https://github.com/nihgwu/react-runner.git"
  },
  "author": "Neo Nie <nihgwu@live.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "component",
    "preview",
    "runner",
    "live"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "cd website && yarn start",
    "deploy": "cd website && yarn deploy",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "clean": "rimraf dist",
    "lint": "eslint 'src/**/*.js'",
    "prettier": "prettier --write '**/src/**/*.{js,css}'"
  },
  "dependencies": {
    "create-react-context": "^0.3.0",
    "prism-react-renderer": "^0.1.6",
    "react-runner": "^0.4.2",
    "react-simple-code-editor": "^0.9.11"
  },
  "peerDependencies": {
    "prop-types": "^15.5.7",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
}
